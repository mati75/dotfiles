XTerm*termName:  xterm-256color
xterm*geometry:           80x25
xterm*faceName:           xft:terminus:pixelsize=7:antialias=false
!!xterm*font:              -*-dina-medium-r-*-*-16-*-*-*-*-*-*-*
!xterm*font: -misc-tamsyn-medium-r-normal--12-87-100-100-c-60-*
xterm*dynamicColors:      true
xterm*utf8:               2
xterm*eightBitInput:      true
xterm*saveLines:          512
xterm*scrollKey:          true
xterm*scrollTtyOutput:    false
xterm*scrollBar:          false
xterm*rightScrollBar:     true
xterm*jumpScroll:         true
xterm*multiScroll:        true
xterm*toolBar:            false
#*background: #262626
#*foreground: #A8A8A8
XTerm*metaSendsEscape: true

urxvt*termName:            xterm-256color
urxvt*scrollBar:        false
URxvt*font:                 xft:Monaco for Powerline:regular:size=10
URxvt*imFont:               xft:Monaco for Powerline:regular:size=10
URxvt*boldFont:             xft:Monaco for Powerline:bold:size=10
URxvt*italicFont:           xft:Monaco for Powerline:italic:size=10
URxvt*boldItalicFont:       xft:Monaco for Powerline:bold:italic:size=10

!URxvt*font:                 xft:Dingbats:pixelsize=7
!URxvt*imFont:               xft:Dingbats:pixelsize=7


!urxvt.font:  xft:Nimbus:pixelsize=7:antialias=false
!URxvt.font:     -misc-tamsyn-medium-r-normal--12-87-100-100-c-60-*
!URxvt.boldfont: -misc-tamsyn-bold-r-normal--12-87-100-100-c-60-*
!URxvt.font: -misc-*-medium-r-normal--15-140-75-75-c-90-iso8859-1

*.background: #232323
*.foreground: #A0A0A0

! Default shell
URxvt*shell: /bin/zsh

! Perl scripts
URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,clipboard-osc,vtwheel

urxvt*matcher.button:    1
URxvt*geometry:            85x25
URxvt.urlLauncher: vimperator2

*.colorBD:    #CFCFCF
*.colorUL:    #A0A0A0
*.colorIT:    #686868

